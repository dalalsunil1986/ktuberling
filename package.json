{
  "name": "ktuberling",
  "version": "0.0.1",
  "description": "KTuberling a simple constructor game suitable for children and adults alike. The idea of the game is based around a once popular doll making concept.",
  "main": "webpack.config.js",
  "scripts": {
    "lint": "eslint src",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-coverage-report": "codecov",
    "build": "webpack --progress --profile --colors",
    "dev": "webpack-dev-server --content-base src",
    "start": "webpack-dev-server --open",
    "watch": "webpack --watch"
  },
  "author": "rumax",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rumax/ktuberling.git"
  },
  "bugs": {
    "url": "https://github.com/rumax/ktuberling.git"
  },
  "homepage": "https://github.com/rumax/ktuberling.git",
  "dependencies": {
    "bootstrap": "3.3.7",
    "debounce": "^1.1.0",
    "extra-log": "^1.0.1",
    "ldjson-stream": "^1.2.1",
    "leaflet": "1.2.0",
    "leaflet-schematic": "1.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "xhr": "^2.4.0",
    "xml-json": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.4",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
  }
}
